{
  "name": "altaTemplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android-dev": "react-native run-android --no-jetifier --variant=devDebug --appId 'com.altaTemplate' ",
    "android-pro": "ENVFILE=.env.pro react-native run-android --no-jetifier --variant=proDebug --appId 'com.altaTemplate.pro' ",
    "ios-dev": "react-native run-ios --scheme EnvDev --configuration DebugDev",
    "ios-pro": "react-native run-ios --scheme EnvPro --configuration DebugPro",
    "detox-build-android": "detox build --configuration android.dev.debug",
    "detox-test-android": "detox test --configuration android.dev.debug",
    "android-dev-release": "npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && cd android && ./gradlew assembleRelease ",
    "android-pro-release": "ENVFILE=.env.pro npx react-native bundle --platform android --dev false --entry-file index.js --bundle-output android/app/src/main/assets/index.android.bundle --assets-dest android/app/build/intermediates/res/merged/release/ && cd android && ENVFILE=.env.pro ./gradlew assembleRelease ",
    "android-code-push-pro": "npx appcenter codepush release-react -a my.bui-alta.com.vn/Testconfig -d Production",
    "android-code-push-dev": "npx appcenter codepush release-react -a my.bui-alta.com.vn/Testconfig -d Staging",
    "clean": "cd android && sh gradlew clean",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .ts,.tsx ",
    "lint:fix": "eslint --fix --ext .ts,.tsx .",
    "postinstall": "husky install"
  },
  "dependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@react-native-async-storage/async-storage": "^1.16.1",
    "@react-navigation/native": "^6.0.8",
    "@react-navigation/native-stack": "^6.5.0",
    "@reduxjs/toolkit": "^1.8.0",
    "@types/react-native-snap-carousel": "^3.8.5",
    "appcenter": "^4.4.3",
    "appcenter-analytics": "^4.4.3",
    "appcenter-crashes": "^4.4.3",
    "axios": "^0.26.1",
    "eslint-plugin-react-native": "^4.0.0",
    "i18n-js": "^3.8.0",
    "lodash": "^4.17.21",
    "moment": "^2.29.1",
    "react": "17.0.2",
    "react-intl": "^5.24.8",
    "react-native": "0.67.3",
    "react-native-animatable": "^1.3.3",
    "react-native-background-timer": "^2.4.1",
    "react-native-code-push": "^7.0.4",
    "react-native-config": "^1.0.0",
    "react-native-fast-image": "^8.6.3",
    "react-native-gesture-handler": "^2.3.2",
    "react-native-linear-gradient": "^2.5.6",
    "react-native-material-ripple": "^0.9.1",
    "react-native-modal": "^13.0.1",
    "react-native-permissions": "^3.3.1",
    "react-native-render-html": "^6.3.4",
    "react-native-responsive-screen": "^1.4.2",
    "react-native-safe-area-context": "^4.1.2",
    "react-native-screens": "^3.13.1",
    "react-native-toast-message": "^2.1.3",
    "react-redux": "^7.2.6",
    "redux": "^4.1.2",
    "redux-logger": "^3.0.6",
    "redux-persist": "^6.0.0",
    "redux-persist-transform-filter": "^0.0.20",
    "sp-react-native-mqtt": "^0.5.1"
  },
  "devDependencies": {
    "@babel/core": "^7.12.9",
    "@babel/plugin-proposal-decorators": "^7.17.2",
    "@babel/runtime": "^7.12.5",
    "@react-native-community/eslint-config": "^2.0.0",
    "@types/jest": "^26.0.23",
    "@types/lodash": "^4.14.179",
    "@types/react-native": "^0.66.15",
    "@types/react-native-background-timer": "^2.0.0",
    "@types/react-native-fetch-blob": "^0.10.7",
    "@types/react-test-renderer": "^17.0.1",
    "@types/redux-logger": "^3.0.9",
    "@typescript-eslint/eslint-plugin": "^5.27.0",
    "@typescript-eslint/parser": "^5.27.0",
    "babel-jest": "^26.6.3",
    "babel-plugin-module-resolver": "^4.1.0",
    "babel-plugin-transform-typescript-metadata": "^0.3.2",
    "detox": "^20.2.0",
    "eslint": "^7.14.0",
    "eslint-plugin-react": "^7.30.0",
    "husky": "^7.0.4",
    "jest": "^29",
    "metro-react-native-babel-preset": "^0.66.2",
    "react-test-renderer": "17.0.2",
    "typescript": "^4.4.4"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
